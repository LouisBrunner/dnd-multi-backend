  "repository": {
    "type": "git",
    "url": "https://github.com/LouisBrunner/react-dnd-multi-backend"
  },
  "keywords": [
    "react",
    "dnd",
    "drag",
    "drop",
    "html5",
    "touch",
    "react-dnd"
